<sub:modifyPortIn>
<sub:mdn><%= mdn %></sub:mdn>
<% if external_port_number %>
<sub:portId><%= external_port_number %></sub:portId>
<% end %>
<% if desired_due_date_time %>
<sub:desiredDueDateTime><%= desired_due_date_time %></sub:desiredDueDateTime>
<% end %>
<sub:timeZone>CST</sub:timeZone>
<sub:billFirstName><%= first_name %></sub:billFirstName>
<sub:billLastName><%= last_name %></sub:billLastName>
<sub:businessName><%= business_name %></sub:businessName>
<sub:billStreetNumber><%= street_number %></sub:billStreetNumber>
<sub:billStreetName><%= street_name %></sub:billStreetName>
<sub:billStreetDirectionIndicator><%= street_direction %></sub:billStreetDirectionIndicator>
<% if city %>
<sub:billCityName><%= city %></sub:billCityName>
<% end %>
<% if state %>
<sub:billStateCode><%= state %></sub:billStateCode>
<% end %>
<sub:zip>
<sub:uspsPostalCd><%= zip %></sub:uspsPostalCd>
</sub:zip>
<% if ssn %>
<sub:ssn><%= ssn %></sub:ssn>
<% end %>
<% if tax_id %>
<sub:taxId><%= tax_id %></sub:taxId>
<% end %>
<sub:accountNumber><%= carrier_account %></sub:accountNumber>
<sub:passwordPin><%= carrier_password %></sub:passwordPin>
<sub:remark><%= remark %></sub:remark>
</sub:modifyPortIn>
